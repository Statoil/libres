QUEUE_SYSTEM LOCAL
QUEUE_OPTION LOCAL MAX_RUNNING 2

NUM_REALIZATIONS 25

DEFINE <STORAGE> storage/<CONFIG_FILE_BASE>

RUNPATH <STORAGE>/runpath/<CASE_NAME>/realisation-%d/iter-%d
ENSPATH <STORAGE>/ensemble
JOBNAME SNAKE_OIL_FIELD

INSTALL_JOB SQUARE_PARAMS jobs/SQUARE_PARAMS

LOAD_WORKFLOW workflows/REALIZATION_NUMBER_WORKFLOW
HOOK_WORKFLOW REALIZATION_NUMBER_WORKFLOW PRE_SIMULATION
LOAD_WORKFLOW_JOB workflows/jobs/REALIZATION_NUMBER

FORWARD_MODEL SQUARE_PARAMS
LOG_LEVEL INFO


